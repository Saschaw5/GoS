{"name":"Dexterous Target. (Drowned Ascetic)","permission":{"default":0,"v18l4Gy2UogedpBX":3},"type":"feat","data":{"description":{"value":"<p>The drowned ascetic adds 3 to its AC against one ranged attack that would hit it. To do so, the drowned ascetic must see the attacker.</p>","chat":"","unidentified":""},"source":"","activation":{"type":"reaction","cost":1,"condition":""},"duration":{"value":0,"units":""},"target":{"value":"","units":"","type":""},"range":{"value":"","long":"","units":""},"uses":{"value":0,"max":0,"per":""},"consume":{"type":"","target":null,"amount":null},"ability":"","actionType":"","attackBonus":0,"chatFlavor":"","critical":null,"damage":{"parts":[],"versatile":""},"formula":"","save":{"ability":"","dc":null,"scaling":"spell"},"requirements":"","recharge":{"value":0,"charged":false}},"flags":{},"img":"modules/gos/characters/202_-_Drowned_Ascetic/avatar.png","_id":"7kiqRYR6h9K8ae66"}
